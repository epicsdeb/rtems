# RTEMS apps expect to find everything under /usr
# including /usr/make
rtems-mvme5500: non-standard-dir-in-usr
rtems-mvme5500: file-in-unusual-dir

# target binaries can be built/linked on any host
rtems-mvme5500: arch-independent-package-contains-binary-or-object

# retain debug symbols.  RTEMS build system automatically strips
# them after final link
rtems-mvme5500: unstripped-binary-or-object

# RTEMS does not support shared libraries
rtems-mvme5500: statically-linked-binary
